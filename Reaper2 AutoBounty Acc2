local First = false
local Target
for _, Player in ipairs(game.Players:GetChildren()) do
    if Player ~= game.Players.LocalPlayer then
        Target = Player
        break
    end
end

if _G.On == nil then 
    _G.On = true
    First = true
else
    if _G.On == true then
     	_G.On = false 
     else
     	 _G.On = true 
     end
end

local plr = game:GetService("Players").LocalPlayer
local tweenService = game:GetService("TweenService")
getgenv().speed = 9e9
local function Punch()
    local ohTable1 = {
    	["inputType"] = Enum.UserInputType.MouseButton1,
    	["keyCode"] = Enum.KeyCode.Unknown}
    game:GetService("ReplicatedStorage").Remotes.Input:FireServer(ohTable1)
    local ohTable1 = {
    	["endedInputType"] = Enum.UserInputType.MouseButton1,
    	["endedKeyCode"] = Enum.KeyCode.Unknown}
    game:GetService("ReplicatedStorage").Remotes.Input:FireServer(ohTable1)
    local ohTable1 = {
    	["endKeyCode"] = Enum.KeyCode.Unknown,
    	["endInputType"] = Enum.UserInputType.MouseButton1}
    game:GetService("ReplicatedStorage").Remotes.Input:FireServer(ohTable1)
end
game.Players.LocalPlayer.Name = "123imnotmomo"
function toTarget(target)
    local speed = getgenv().speed
    local info = TweenInfo.new((target.Position - plr.Character.HumanoidRootPart.Position).Magnitude / speed, Enum.EasingStyle.Linear)
    local _, err = pcall(function()
        tweenService:Create(plr.Character.HumanoidRootPart, info, {CFrame = target*CFrame.new(0, 0, 1.5)}):Play()
    end)
    if err then error("Couldn't create/start tween: ", err) end
end
function newIndexHook()
    local mt = getrawmetatable(game)
    local oldIndex = mt.__newindex
    setreadonly(mt, false)
    mt.__newindex = newcclosure(function(self, i, v)
        if checkcaller() and self == plr.Character.HumanoidRootPart and i == 'CFrame' then
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
            return toTarget(v) 
        end
        return oldIndex(self, i, v)
    end)
    setreadonly(mt, true)
end
newIndexHook()
if First == true then 
    toTarget(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, -500, 0))
    game.Players.LocalPlayer.CharacterAdded:Connect(function()
        if _G.On == true then
            game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
            toTarget(game.Players[Target.Name].Character.HumanoidRootPart.CFrame)
            wait(0.07)
            Punch()
            wait(0.5)
            local Running = true
            game.Players.LocalPlayer.CharacterRemoving:Connect(function()
                Running = false
            end)
            while Running == true do
                toTarget(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame*CFrame.new(0, 0, 50))
                wait(0.1)
                for count = 1, 5 do 
                    game.Players.LocalPlayer.Character.Humanoid.Health = 0
                end
                wait(2)
            end
        end
    end)
end
